import java.util.*;
class Codechef
{
	public static void main (String[] args)
	{
		Scanner sc = new Scanner(System.in);
        int test = sc.nextInt();
        while (test-- > 0) {
            int n1 = sc.nextInt();
            SortedSet<Integer> s2 = new TreeSet<>();
            for (int i = 0; i<n1; i++)
                s2.add(sc.nextInt());
            s2.remove(0);
            System.out.println(s2.size());
        }
	}
}
